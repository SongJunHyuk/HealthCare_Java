
public class StringTest {

	public static void main(String[] args) {
	      String s1 = "Hello"; // 리터럴
	      String s2 = "Hello"; // 리터럴
	      String s3 = new String("Hello");
	      String s4 = new String("Hello");
	      
	      if( s1 == s2 ) System.out.println("s1 == s2");
	      if( s3 == s4 ) System.out.println("s3 == s4"); // false
	      if( s1 == s3 ) System.out.println("s1 == s3"); // false

	      if( s1.equals(s2) )System.out.println("s1 equal s2"); // true
	      if( s3.equals(s4) )System.out.println("s3 equal s4"); // true
	      if( s1.equals(s4) )System.out.println("s1 equal s4"); // true

	}
}