
public class Startree {

	public static void main(String[] args) {
	      // nested loop
	      for (int i = 0; i < 5; i++) {
	         
	         for (int j = 0; j < 5; j++) {
	            if( j <= i ) System.out.print('*'); 
	         }
	         System.out.println();
	      }
	      
	      // 좀 더 효율적인 코드
	      for (int i = 0; i < 5; i++) {
	         
	         for (int j = 0; j <= i; j++) {
	            System.out.print('*'); 
	         }
	         System.out.println();
	      }
	}
}